{
  "name": "@sidhujag/sysweb3",
  "version": "1.0.34",
  "description": "Syscoin Web3 JavaScript API",
  "author": "Pali Wallet",
  "email": "support@syscoin.org",
  "license": "MIT",
  "homepage": "https://github.com/syscoin/sysweb3#readme",
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "private": true,
  "workspaces": [
    "packages/*/dist/*"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "build:all": "cd packages/sysweb3-core && yarn publishCode && cd ../sysweb3-network && yarn publishCode && cd ../sysweb3-utils && yarn publishCode && cd ../sysweb3-keyring && yarn publishCode",
    "publish:all": "cd packages/sysweb3-core && yarn release:npm && cd ../sysweb3-network && yarn release:npm && cd ../sysweb3-utils && yarn release:npm && cd ../sysweb3-keyring && yarn release:npm"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syscoin/sysweb3.git"
  },
  "bugs": {
    "url": "https://github.com/syscoin/sysweb3/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^12.4.1",
    "minimist": "1.2.6",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "wif": "^5.0.0"
  }
}
