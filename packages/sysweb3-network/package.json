{
  "name": "@sidhujag/sysweb3-network",
  "version": "1.0.101",
  "description": "A network management tool for multi-chain accounts.",
  "main": "cjs/index.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syscoin/sysweb3.git"
  },
  "author": "Pali Wallet",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/syscoin/sysweb3/issues"
  },
  "homepage": "https://github.com/syscoin/sysweb3#readme",
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "tsc && tsc -p tsconfig.json",
    "prerelease": "yarn build",
    "copy": "node ../../scripts/package/copy",
    "release:npm": "yarn publish ./dist --access=public --no-git-tag-version",
    "release:preflight": "yarn pack ./dist --dry-run",
    "publishCode": "yarn prebuild && yarn prerelease && yarn copy && yarn release:npm",
    "type-check": "tsc --pretty --noEmit",
    "localTest": "yarn prebuild && yarn build && yarn copy"
  },
  "dependencies": {
    "eth-chains": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "ethers": "~5.7.0"
  }
}